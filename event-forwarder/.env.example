# Example environment variables for event-forwarder
# Copy this to .env and fill in your values

# Your DeepFry relay WebSocket URL
DEEPFRY_RELAY_URL=wss://your-deepfry-relay.com

# Nostr secret key for signing sync progress events
# Can be nsec format or 32-byte hex
NOSTR_SYNC_SECKEY=nsec1your_secret_key_here

# Optional: Version info for Docker builds
VERSION=latest
GIT_COMMIT=dev
BUILD_TIME=unknown
